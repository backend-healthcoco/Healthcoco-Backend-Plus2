	<profiles>
		<profile>
			<id>local</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<build>
				<finalName>dpdocter</finalName>
				<resources>
					<resource>
						<directory>src/main/resources/spring-context</directory>
						<filtering>true</filtering>
					</resource>
					<resource>
						<directory>src/main/resources/spring-properties/local</directory>
						<filtering>true</filtering>
					</resource>
					<resource>
						<directory>src/main/resources/velocityTemplates</directory>
						<filtering>true</filtering>
					</resource>
				</resources>
			</build>
		</profile>
